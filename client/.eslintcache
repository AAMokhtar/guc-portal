Schedule(schedule);\r\n      let schedule2 = (\r\n        <a class=\"btn btn-info\" onClick={() => this.handleSchedule(schedule)}>\r\n          Info\r\n        </a>\r\n      );\r\n\r\n      data.push({\r\n        ...el,\r\n        staffID: id,\r\n        schedule: schedule2,\r\n        courseIDs: courses,\r\n      });\r\n    });\r\n    return (\r\n      <div className=\"App\">\r\n        <p className=\"Table-header\">Staff Profile</p>\r\n\r\n        <BootstrapTable keyField=\"id\" data={data} columns={columns} />\r\n      </div>\r\n    );\r\n    /*  return (\r\n      <div>\r\n        <h1>12</h1>\r\n      </div>\r\n    ); */\r\n  }\r\n}\r\n\r\nexport default ViewProfiles;\r\n","D:\\Desktop\\Assignments\\2020-2021\\ACL2\\guc-portal\\client\\src\\components\\cards\\cards.jsx",[],"D:\\Desktop\\Assignments\\2020-2021\\ACL2\\guc-portal\\client\\src\\components\\staffManagement\\staffService.js",[],"D:\\Desktop\\Assignments\\2020-2021\\ACL2\\guc-portal\\client\\src\\util\\parseSchedule.js",["323","324","325","326","327","328"],"export function parseSchedule(schedule) {\r\n  try {\r\n    let sat = new Object();\r\n    let sun = new Object();\r\n    let mon = new Object();\r\n    let tue = new Object();\r\n    let wed = new Object();\r\n    let thu = new Object();\r\n\r\n    //sort the schedule\r\n    schedule.forEach((slot) => {\r\n      if (slot.weekday === \"Saturday\") {\r\n        if (slot.number === \"First\") {\r\n          sat.first = slot;\r\n        } else if (slot.number === \"Second\") {\r\n          sat.second = slot;\r\n        } else if (slot.number === \"Third\") {\r\n          sat.third = slot;\r\n        } else if (slot.number === \"Fourth\") {\r\n          sat.fourth = slot;\r\n        } else if (slot.number === \"Fifth\") {\r\n          sat.fifth = slot;\r\n        }\r\n      } else if (slot.weekday === \"Sunday\") {\r\n        if (slot.number === \"First\") {\r\n          sun.first = slot;\r\n        } else if (slot.number === \"Second\") {\r\n          